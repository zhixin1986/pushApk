{
    "go.toolsManagement.autoUpdate": true,
    "go.useLanguageServer": true,
    "go.lintOnSave": "package",
    "go.formatTool": "gofmt",
    "go.delveConfig": {
        "dlvLoadConfig": {
            "followPointers": true,
            "maxVariableRecurse": 1,
            "maxStringLen": 64,
            "maxArrayValues": 64,
            "maxStructFields": -1
        },
        "apiVersion": 2,
        "showGlobalVariables": true
    }
}
